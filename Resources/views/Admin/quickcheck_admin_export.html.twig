<h3>
    <span class="fa fa-edit"></span>
    {{ "Export Questions"|trans }}
</h3>
<p>{{ "Pick the questions that you would like to export."|trans }}</p>
{{ showflashes() }}
        {{ form_start(form) }}
        {{ form_errors(form) }}
<fieldset>
    <legend>{{ 'Questions'|trans }}</legend>
    {{ questions|raw }}
</fieldset>
<div class="form-group">
    <div class="col-lg-offset-3 col-lg-9">
        {{ form_widget(form.export, {'attr': {'class': 'btn btn-success'}}) }}
        {{ form_widget(form.exportall, {'attr': {'class': 'btn btn-success'}}) }}
    </div>
</div>
{{ form_end(form) }}
